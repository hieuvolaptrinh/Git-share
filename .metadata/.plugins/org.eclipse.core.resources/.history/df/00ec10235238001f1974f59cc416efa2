import java.util.Scanner;

public class vidu {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n;
		System.out.println("nhap so lan can chep phat");
		n = sc.nextInt();

		for (int i = 0; i < n; i++) {
			System.out.println("em hứa sẽ học bài và làm bài tập đầy đủ");

		}
	}
}
